public class Main {
    public static void main(String[] args) {
    
        int[] nums = {0, 1, 2, 2,6,1,45,12, 3, 0, 4, 2};
        int n = nums.length;
        int[] arr = new int[n];
        int target = 2;
        int i , j = 0;
        for(i = 0; i < n; i++)
        {
            if(nums[i] == target)
                continue;
            else
                arr[j++]=nums[i];
        }
        System.out.println(j);
        }
    }
